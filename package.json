{
  "name": "@ww/llm-gql-service",
  "version": "1.47.0",
  "description": "example GQL Service",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --testTimeout 60000",
    "test:integration": "NODE_ENV=test jest --config ./jest.integration.config.js --testTimeout 60000",
    "test:watch": "npm run test -- --watch",
    "build": "babel src --out-dir lib --extensions '.js,.ts' --source-maps inline  --copy-files",
    "start": "node .",
    "start:dev": "nodemon --watch src --ext 'ts,json,js' --exec 'npm run build && npm run start'",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "prettier:all": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepublish": "rimraf lib && npm run build",
    "prepare": "husky"
  },
  "author": "Windward ltd.",
  "dependencies": {
    "@langchain/community": "^0.2.1",
    "@langchain/core": "^0.2.17",
    "@ww/gql-base-service": "^3.225.0",
    "@ww/gql-schema-types": "^1.555.0",
    "@ww/js-core": "^1.325.0",
    "@ww/node-utils": "^3.180.0",
    "dotenv": "16.3.1",
    "duck-duck-scrape": "^2.2.5",
    "js-yaml": "^4.1.0",
    "langchain": "^0.1.35",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@ww/eslint-config": "^1.18.0",
    "@ww/node-testing-library": "^1.5.0",
    "@ww/prettier-config": "^1.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-watch": "^7.7.0",
    "eslint": "^8.16.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^9.0.11",
    "jest": "^29.0.0",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.2"
  },
  "prettier": "@ww/prettier-config",
  "lint-staged": {
    "!*.{ts,js}": "prettier --ignore-unknown --write",
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
